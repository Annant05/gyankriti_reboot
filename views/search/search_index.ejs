<!DOCTYPE html>
<html lang="en">

<!--suppress HtmlRequiredTitleElement -->
<head>

    <% include ../partials/head.ejs %>

</head>

<body>

<div class="wrapper">

    <!-- Page Content Holder -->
    <div id="content">
        <!--Page Navbar start-->
        <% include ../partials/navbar.ejs %>
        <!--Page Navabar end -->

        <div class="container parent-container" id="main_content_container">

            <div class="container child-container" id="admission_form_container">

                <div class="row">
                    <div class="col-md-12">
                        <div class="float-left"><h1>Search</h1></div>

                    </div>
                </div>

                <hr>

                <div class="row">
                    <div class="container">
                        <div class="height-content">

                            <form id="search_for" novalidate="">

                                <div class="form-row">
                                    <div class="form-group col-md-3 text-center">
                                        <label for="inputstu_standard">Search For :</label>

                                        <select id="inputstu_standard"
                                                class="text-bold form-control form-control-md">
                                            <option value="select" selected>Select...</option>

                                            <option value="standard-1">Standard-1</option>
                                            <option value="standard-2">Standard-2</option>
                                            <option value="standard-3">Standard-3</option>
                                            <option value="standard-4">Standard-4</option>
                                            <option value="standard-5">Standard-5</option>
                                            <option value="standard-6">Standard-6</option>

                                        </select>

                                    </div>

                                    <div class="form-group col-md-3 text-center">
                                        <label for="inputstu_section">Search By :</label>

                                        <select id="inputstu_section"
                                                class="text-bold form-control form-control-md">
                                            <option value="select" selected>Select...</option>

                                            <option value="A">A</option>
                                            <option value="B">B</option>
                                            <option value="C">C</option>

                                        </select>
                                    </div>

                                    <div class="form-group col-md-3 text-center">
                                        <label for="inputstu_vanroute">Van Route</label>

                                        <select id="inputstu_vanroute"
                                                class="text-bold form-control form-control-md">
                                            <option value="select" selected>Select...</option>

                                            <option value="5A">5A</option>
                                            <option value="1A">1A</option>
                                            <option value="2C">2C</option>

                                        </select>
                                    </div>

                                    <div class="form-group col-md-3 text-center">
                                        <label for="inputstu_vanshift">Van Shift</label>

                                        <select id="inputstu_vanshift"
                                                class="text-bold form-control form-control-md">
                                            <option value="select" selected>Select...</option>

                                            <option value="1">1</option>
                                            <option value="2">2</option>
                                            <option value="3">3</option>

                                        </select>
                                    </div>


                                </div>
                                <!--<button type="submit" class="btn btn-primary button-shadow">Sign in</button>-->
                            </form>
                        </div>

                        <hr>

                    </div>
                </div>
            </div>


        </div>
    </div>
</div>

<!-- Include all the jscripts here -->
<% include ../partials/js_bottom.ejs %>
<script src="search_index.js"></script>
<!--End all the jscripts above-->
</body>

</html>